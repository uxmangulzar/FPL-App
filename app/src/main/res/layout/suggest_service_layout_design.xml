<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/cardbg">
    <RelativeLayout
        android:id="@+id/sq_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <RelativeLayout
            android:id="@+id/cancel_dialog"
            android:layout_width="@dimen/dp50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp12"
            android:layout_marginTop="@dimen/dp22">
            <ImageView
                android:layout_width="@dimen/dp16"
                android:layout_height="@dimen/dp16"
                android:layout_centerHorizontal="true"
                android:src="@drawable/close">
            </ImageView>
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp14"
            android:fontFamily="@font/brotherbold"
            android:text="Suggest Service"
            android:textColor="@color/qty_color"
            android:textSize="20sp">
        </TextView>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/service_list_btn"
        android:background="@drawable/button_add_ticket"
        android:layout_marginTop="64dp"
        android:layout_marginStart="23dp"
        android:layout_marginEnd="23dp"
        android:layout_below="@+id/q_btn_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50">
        <TextView
            android:textColor="@color/white"
            android:textSize="15sp"
            android:fontFamily="@font/robotoregular"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Service List">
        </TextView>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/get_all_store_btn"
        android:background="@drawable/edittext_colorfull_join_fpl"
        android:layout_marginTop="15dp"
        android:layout_marginStart="23dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="@dimen/dp16"
        android:layout_below="@+id/service_list_btn"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50">
        <TextView
            android:textColor="@color/colorPrimary"
            android:textSize="15sp"
            android:fontFamily="@font/robotoregular"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Store">
        </TextView>
    </RelativeLayout>
</RelativeLayout>